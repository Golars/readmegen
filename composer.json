{
  "name": "golars/readmegen",
  "type": "application",
  "description": "Readme file / doc generator. It uses VCS logs as a source of information.",
  "keywords": [
    "readme",
    "generator",
    "log parser",
    "vcs"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vitaliy",
      "email": "vipaware@gmail.com"
    },
    {
      "name": "Kamil Fojuth",
      "email": "fojuth@gmail.com"
    },
    {
      "name": "Voodoo Mobile",
      "email": "public@voodoo-mobile.com"
    }
  ],
  "require": {
    "symfony/yaml": "^3.0 || ~2.1",
    "ulrichsg/getopt-php": "2.*"
  },
  "require-dev": {
    "phpspec/phpspec": "dev-master"
  },
  "bin": ["readmegen"],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "ReadmeGen\\": "src/"
    }
  }
}
